{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@drag-drop": ["./src/drag-drop/dag-drop.module"],
      "@drag-drop/*": ["./src/drag-drop/*"],

    },
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
